<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context-4.0.xsd">
        <bean id="mainView" class="com.hdoubleq.view.MainView" scope="singleton"></bean>
        <bean id="viewService" class="com.hdoubleq.view.Service.ViewService"></bean>
        <bean id="customerListModel" class="com.hdoubleq.model.CustomerListModel" scope="prototype"></bean>
        <bean id="linkServiceDao" class="com.hdoubleq.dao.LinkServiceDao"/>
        <bean id="sendToSercice" class="com.hdoubleq.dao.SendToSercice" scope="prototype"/>
        <bean id="receiveFromService" class="com.hdoubleq.dao.ReceiveFromService" />

</beans>